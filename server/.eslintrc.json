{
  "rules": {
    "linebreak-style": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "max-len": [
      "error",
      {
        "ignoreComments": true
      }
    ],
    "no-param-reassign": [
      "error",
      {
        "props": true,
        "ignorePropertyModificationsFor": [
          "moduleState"
        ]
      }
    ]
  }
}